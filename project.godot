config_version=5

[application]

config/name="Twin Blade Tango"
config/description="With one blade between them a couple fight for their love"
config/version="0.1"
run/main_scene="res://System/Screens/Start_Screen.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.png"

[autoload]

PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
SimpleGrass="*res://addons/simplegrasstextured/singleton.tscn"
FullscreenToggle="*res://System/fullscreen_toggle.gd"

[debug]

gdscript/warnings/standalone_expression=2
gdscript/warnings/untyped_declaration=1

[editor_plugins]

enabled=PackedStringArray("res://addons/simplegrasstextured/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "gray"
}

[global_group]

Player="A player character"

